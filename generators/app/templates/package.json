{
  "name": "<%= projectName.replace(/ /ig,'') %>",
  "version": "0.1.0",
  "devDependencies": {
    "browser-sync": "latest",
    "del": "latest",
    "emitty": "1.4.0",
    "foldero": "latest",
    "gulp-cheerio": "latest",
    "gulp-data": "latest",
    "gulp-imagemin": "latest",
    "gulp-pug": "latest",
    "gulp-watch": "latest",
    "imagemin-jpegoptim": "latest",
    "imagemin-pngquant": "latest",
    "pug": "latest",
    "pug-include-glob": "latest"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "www",
      "otherWWW": [],
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "assets": "assets",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_img",
      "icons": "_icons",
      "fonts": "_fonts",
      "styles": "_css",
      "scripts": "_js",
      "data": "_data"
    },
    "entries": {
      "js": "main.js",
      "css": "main**.{sass,scss}"
    }
  },
  "dependencies": {
	"autobind-decorator": "^2.1.0",
    "autoprefixer": "latest",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-minify-webpack-plugin": "^0.2.0",
	"babel-plugin-add-module-exports": "^0.2.1",
	"babel-plugin-transform-class-properties": "^6.24.1",
	"babel-plugin-transform-decorators-legacy": "^1.3.4",
	"babel-plugin-transform-runtime": "^6.23.0",
	"babel-preset-es2015": "^6.22.0",
	"babel-preset-react": "^6.24.1",
	"babel-preset-stage-1": "^6.24.1", <% if (cssFramework == 'bootstrap') { %>
    "bootstrap-sass": "latest", <% } %>
	"bootstrap-select": "latest",
	"conditioner-core": "^2.3.1",<% if (cssFramework == 'foundation') { %>
    "foundation-sites": "latest",<% } %>
    "gulp": "latest",
    "gulp-changed": "latest",
    "gulp-check-filesize": "latest",
    "gulp-consolidate": "latest",
    "gulp-css-globbing": "latest",
    "gulp-cssnano": "latest",
    "gulp-eslint": "latest",
    "gulp-iconfont": "latest",
    "gulp-if": "latest",
    "gulp-load-plugins": "latest",<% if(useNotif) {%>
    "gulp-notify": "latest",<% } %>
    "gulp-plumber": "latest",
    "gulp-postcss": "latest",
    "gulp-rename": "latest",
    "gulp-replace": "latest",
    "gulp-sass": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-util": "latest",
	"hamburgers": "latest",
	"headroom.js": "^0.9.4",
	"jquery-match-height": "latest",
	"jquery.scrollto": "^2.1.2",
	"json5": "latest",
	"lazysizes": "^4.0.1",
	"lodash": "latest",
	"magnific-popup": "^1.1.0",
    "name-all-modules-plugin": "latest",
    "postcss-assets": "latest",
    "postcss-critical-split": "latest",
    "postcss-pxtorem": "latest",
    "postcss-sprites": "latest",
    "rucksack-css": "latest",
    "run-sequence": "latest",
    "simplestatemanager": "latest",
    "slick-carousel": "latest", <% if (vue) { %>
    "vue-loader": "latest",
    "vue-template-compiler": "latest", <% } %>
    "webpack": "^3.5.6",
    "webpack-stream": "^3.2.0",
    "wrench": "latest"
  },
  "resolutions":{
	  "webpack/**/webpack-sources": "1.0.1",
	  "babel-minify-webpack-plugin/**/webpack-sources": "1.0.1"
  }
}
